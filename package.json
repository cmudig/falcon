{
  "name": "falcon-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Falcon package and examples of cross-filtering",
  "repository": "git+https://github.com/cmudig/falcon.git",
  "author": "xnought <bertuccd@oregonstate.edu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmudig/falcon/issues"
  },
  "homepage": "https://github.com/cmudig/falcon#readme",
  "scripts": {
    "start:flights": "yarn workspace flights dev --open",
    "start:movies-arrow": "yarn workspace movies-arrow dev --open",
    "start:movies-duckdb": "yarn workspace movies-duckdb dev --open",
    "start:movies-json": "yarn workspace movies-json dev --open",
    "dev": "yarn start:movies-duckdb",
    "build": "yarn workspace falcon2 build",
    "format": "yarn workspace falcon2 format",
    "test": "yarn workspace falcon2 test",
    "build:deploy": "yarn workspace movies-duckdb build",
    "deploy": "gh-pages -d examples/movies-duckdb/dist",
    "pub": "yarn workspace falcon2 pub"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  },
  "directories": {
    "examples": "./examples/*",
    "falcon2": "./falcon2"
  },
  "workspaces": [
    "falcon2",
    "examples/*"
  ]
}
